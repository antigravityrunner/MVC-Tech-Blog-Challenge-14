<h2>Dashboard</h2>

{{#each posts as |post|}}
  <div class="row align-center mb-5">
    <div class="col-md-6">
      <h2>{{post.title}}</h2>
    </div>
    <div class="col-md-6">
      <h3><a href="">{{post.title}}</a></h3>
    </div>
  </div>
{{/each}}

<a href="/posts/new"><button class="btn btn-primary">Create new Post</button></a>